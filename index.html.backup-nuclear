  <script>
      function initColliderNavTabsDetection() {
      const targetUrls = [
        'https://www.drinkcollider.com/products/unwind-mixed-1',
        'https://www.drinkcollider.com/products/unwind-lager-1', 
        'https://www.drinkcollider.com/products/unwind-session-pale-1'
      ];
      
      if (!targetUrls.some(url => window.location.href.includes(url))) return;
      
      window.navTabsReached = sessionStorage.getItem('navTabsReached') === 'true' || false;
      
      function setupObserver() {
        const navElement = document.querySelector('.nav.nav-tabs');
        if (!navElement) {
          setTimeout(setupObserver, 500);
          return;
        }
        
        const observer = new IntersectionObserver((entries) => {
          entries.forEach(entry => {
            if (entry.isIntersecting && !window.navTabsReached) {
              window.navTabsReached = true;
              sessionStorage.setItem('navTabsReached', 'true');
            }
          });
        }, { threshold: 0.1 });
        
        observer.observe(navElement);
      }
      
      if (document.readyState === 'loading') {
        document.addEventListener('DOMContentLoaded', setupObserver);
      } else {
        setupObserver();
      }
    }

    initColliderNavTabsDetection();
  </script>

</body></html>