<script>
  document.addEventListener('DOMContentLoaded', function() {
   const badges = document.querySelectorAll('.js-product-item-badge');
    badges.forEach(function(badge) { 
      const rate = badge.querySelector('.jdgm-prev-badge').getAttribute('data-average-rating');
      const roundedRate = Math.round(rate * 10) / 10; 

      if (roundedRate) {
        badge.querySelector('.js-product-item-rate').textContent = roundedRate + '/5';
      } else {
        badge.querySelector('.js-product-item-rate').textContent = '';
      }
  });
  });
</script>

