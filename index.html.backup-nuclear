<script>
  function isHeaderOutOfView() {
  const indexHeader = document.querySelector('.js-index-header');
   
  const rect = indexHeader.getBoundingClientRect();
  return rect.bottom <= 0;
  }

  function toggleHeaderVisibleClass() {
  if (window.innerWidth > 1199) return false;  
  const root = document.body; 
  
  if (isHeaderOutOfView()) {
    root.classList.remove('index-header-visible');
  } else {
    root.classList.add('index-header-visible');
  }
  }

  window.addEventListener('scroll', toggleHeaderVisibleClass);
  window.addEventListener('load', toggleHeaderVisibleClass);
  window.addEventListener('resize', toggleHeaderVisibleClass);

</script>

